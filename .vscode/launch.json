  "version": "0.2.0",
  "compounds": [
    {
      "name": "Launch & Debug",
      "configurations": [
        "Launch Program",
        "Launch Chrome against localhost"
      ]
    }
  ],
  "configurations": [
    {
      "type": "php",
      "request": "launch",
      "name": "Launch Program",
      "cwd": "${workspaceRoot}",
      "port": 9000
    },
    {
      "name": "Launch Chrome against localhost",
      "type": "chrome",
      "request": "launch",
      "url": "http://appsekolah.test/",
      "webRoot": "${workspaceRoot}"
    }
  ]
}